generator client {
  provider = "prisma-client-js"
}

datasource bd {
  provider  = "postgresql"
  url       = env("POSTGRES_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id Int @id @default(autoincrement())

  fulName  String
  email    String
  password String

  createdAt DateTime @default(now())
  updatedAt DateTime
}

model Product {
  id Int @id @default(autoincrement())

  name     String
  imageUrl String

  createdAt DateTime @default(now())
  updatedAt DateTime
}
